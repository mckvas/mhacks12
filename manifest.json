{
    "name": "Dude",
    "version": "1.0",
    "description": "Build an Extension!",
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "web_accessible_resources": [
      "images/judging.jpg"
  ],
 
 "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css":["image.css"]
   }],
    "manifest_version": 2,
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "images/bojack16.png",
      "default_title": "Bojack gon remind ya"
    },
    "permissions": ["activeTab", "storage"]
}
